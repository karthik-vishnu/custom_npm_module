{
    "presets": [
      "latest",
      "react",
      "stage-1"
    ],
    "env": {
      "localhost": { 
        "presets": [ 
          "react", 
          "stage-1", 
          "es2015"
        ] 
      },
      "development": {
        "presets": [
          "latest",
          "react-hmre"
        ]
      },
      "production": {
        "presets": [
          ["latest", {
            "es2015": {
              "modules": false
            }
          }]
        ],
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-remove-prop-types"
        ]
      },
      "test": {
        "presets": [
          "latest"
        ]
      }
    }
  }
  
  